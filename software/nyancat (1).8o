	  v2 := random 0b11
	  v3 := random 0b11111
		i := stars i += v4  save v3
		v4 += 4  if v4 != 32 then
	again
	
  loadSong 0 0

	loop  
	  i := overalltick  load v1  v1 += 1  if v1 == 0 then  v0 += 1  
