:calc .... { 0b0000 } :calc ...@ { 0b0001 } :calc @... { 0b1000 } :calc @..@ { 0b1001 }
:calc ..@. { 0b0010 } :calc ..@@ { 0b0011 } :calc @.@. { 0b1010 } :calc @.@@ { 0b1011 }
:calc .@.. { 0b0100 } :calc .@.@ { 0b0101 } :calc @@.. { 0b1100 } :calc @@.@ { 0b1101 }
:calc .@@. { 0b0110 } :calc .@@@ { 0b0111 } :calc @@@. { 0b1110 } :calc @@@@ { 0b1111 }

:calc ra { @@@. }

:macro 8c AA BB CC DD EE FF GG HH {
    :calc ra { AA }  :calc rb { BB } :calc rc { CC }  :calc rd { DD }
    :calc re { EE }  :calc rf { FF } :calc rg { GG }  :calc rh { HH }
}
:macro 8c_init {
    :calc tra { 0 } :calc trb { 0 } :calc trc { 0 } :calc trd { 0 }
    :calc tre { 0 } :calc trf { 0 } :calc trg { 0 } :calc trh { 0 }
}
:macro 8c_sto {
    :calc tra { ( tra << 4 ) | ra } :calc trb { ( trb << 4 ) | rb }
    :calc trc { ( trc << 4 ) | rc } :calc trd { ( trd << 4 ) | rd }
    :calc tre { ( tre << 4 ) | re } :calc trf { ( trf << 4 ) | rf }
    :calc trg { ( trg << 4 ) | rg } :calc trh { ( trh << 4 ) | rh }
}

:macro 8c_byta { :byte { ( tra >> 24 ) & 240 } :calc tra { tra << 4 } }
:macro 8c_bytb { :byte { ( trb >> 24 ) & 240 } :calc trb { trb << 4 } }
:macro 8c_bytc { :byte { ( trc >> 24 ) & 240 } :calc trc { trc << 4 } }
:macro 8c_bytd { :byte { ( trd >> 24 ) & 240 } :calc trd { trd << 4 } }
:macro 8c_byte { :byte { ( tre >> 24 ) & 240 } :calc tre { tre << 4 } }
:macro 8c_bytf { :byte { ( trf >> 24 ) & 240 } :calc trf { trf << 4 } }
:macro 8c_bytg { :byte { ( trg >> 24 ) & 240 } :calc trg { trg << 4 } }
:macro 8c_byth { :byte { ( trh >> 24 ) & 240 } :calc trh { trh << 4 } }

:macro 8c_fin6 { 8c_sto  8c_sto
    8c_byta 8c_byta 8c_byta 8c_byta 8c_byta 8c_byta 
    8c_bytb 8c_bytb 8c_bytb 8c_bytb 8c_bytb 8c_bytb 
    8c_bytc 8c_bytc 8c_bytc 8c_bytc 8c_bytc 8c_bytc 
    8c_bytd 8c_bytd 8c_bytd 8c_bytd 8c_bytd 8c_bytd 
    8c_byte 8c_byte 8c_byte 8c_byte 8c_byte 8c_byte 
    8c_bytf 8c_bytf 8c_bytf 8c_bytf 8c_bytf 8c_bytf 
    8c_bytg 8c_bytg 8c_bytg 8c_bytg 8c_bytg 8c_bytg 
		8c_byth 8c_byth 8c_byth 8c_byth 8c_byth 8c_byth 
}

