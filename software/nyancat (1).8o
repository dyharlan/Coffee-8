: flytext_draw
	load v3  i := flytext_str_ref  save v1  v1 := 0  v5 := 0
	:next flytext_x_offset  v2 += 0
	:next flytext_y_offset  v3 += 0
	:next flytext_z_offset  v5 += 0
	i := overalltick  load v7 - v8
	v3 <<= v3  if vF != 0 then return
	v3 <<= v3  if vF != 0 then return
	v3 >>= v3  v3 >>= v3  v4 := v3
	loop
		0xF0 0  : flytext_str_ref  0 0
		i += v1  load v0  if v0 == 0xFF then ;
		i := long sintable  v7 := v8 v7 += v7   v7 += v5  v7 += v5  v7 += v5  i += v7  load v7 - v7
		v3 := v4  v7 >>= v7  v7 >>= v7  v7 >>= v7  v7 >>= v7   v7 >>= v7  v7 >>= v7  v3 += v7  v3 -= 2
		plane 1  i := long fontSet i += v0 i += v0  sprite v2 v3 6
		plane 2  i := long fontSha i += v0 i += v0  sprite v2 v3 6
	v1 += 1  v5 += 1  v2 += 4  again

